{
  "displayName": "Data Managment Demo Dataset",
  "name": "data-management-demo-dataset",
  "description": "This dataset serves as a practical tool for users to familiarize themselves with the fundamental functionalities of various tabs in the dataset viewer.",
  "scope": "public",
  "version": "0.0.7",
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/data-management-demo-dataset.git",
    "gitTag": "0.0.7"
  },
  "attributes": {
    "Category": "Dataset",
    "Hub": "Dataloop",
    "Provider": "Dataloop",
    "Media Type": "Image",
    "Annotation Type": "Bounding Box"
  },
  "metadata": {
    "system": {
      "systemCompute": true
    }
  },
  "components": {
    "modules": [
      {
        "name": "dataset-adapter",
        "entryPoint": "dataset_loader.py",
        "className": "DatasetExample",
        "description": "Dataset adapter for Dataloop Example",
        "initInputs": [],
        "functions": [
          {
            "name": "upload_dataset",
            "input": [
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "Dataloop Dataset Entity"
              },
              {
                "type": "String",
                "name": "source",
                "description": "Dataset source URL"
              }
            ],
            "output": [],
            "displayName": "upload_dataset",
            "displayIcon": "",
            "description": "function to upload dataset to Dataloop"
          }
        ]
      }
    ],
    "datasets": [
      {
        "name": "Data Managment Demo Dataset",
        "docs": "https://github.com/dataloop-ai-apps/data-management-demo-dataset",
        "source": "https://github.com/dataloop-ai-apps/data-management-demo-dataset",
        "invoke": {
          "namespace": "dataset-adapter.upload_dataset"
        },
        "computeConfig": "data-management-demo-dataset-deploy"
      }
    ]
  }
}
